{
  "name": "resource-pack-backporter",
  "version": "1.0.0",
  "description": "Universal Minecraft resource pack backporter from 1.21.4+ to 1.21.1 using CIT + Pommel",
  "main": "resource-pack-backporter.ts",
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "dev": "bun run resource-pack-backporter.ts",
    "build": "bun build resource-pack-backporter.ts --outdir dist --target bun",
    "lint": "bunx @biomejs/biome check .",
    "format": "bunx @biomejs/biome format --write ."
  },
  "keywords": ["minecraft", "resource-pack", "backport", "cit", "pommel", "1.21.4", "1.21.1"],
  "author": "Better Fresher 3D Books Backporter",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "bun-types": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
